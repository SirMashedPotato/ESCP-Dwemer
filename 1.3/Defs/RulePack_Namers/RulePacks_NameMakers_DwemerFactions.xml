<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ESCP_NamerFactionDwemer</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[a][b]'s Kingdom</li>
      </rulesStrings>
	  <rulesFiles>
		<li>a->ESCP_DwemerNames/DwemerName_A</li>
		<li>b->ESCP_DwemerNames/DwemerName_B</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

  <RulePackDef>
    <defName>ESCP_NamerSettlementDwemerEastern</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[name]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>name->ESCP_DwemerNames/DwemerCities_Eastern</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>
  
  <RulePackDef>
    <defName>ESCP_NamerSettlementDwemerWestern</defName>
    <rulePack>
      <rulesStrings>
        <li>r_name->[name]</li>
      </rulesStrings>
	  <rulesFiles>
        <li>name->ESCP_DwemerNames/DwemerCities_Western</li>
      </rulesFiles>
    </rulePack>
  </RulePackDef>

</Defs>
