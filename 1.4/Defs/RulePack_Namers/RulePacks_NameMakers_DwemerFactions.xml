	<RulePackDef>
		<defName>ESCP_Chimer_NamerFaction</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->[a][b]'s Kingdom</li>
			</rulesStrings>
			<rulesFiles>
				<li>a->ESCP_DwemerNames/DwemerName_A</li>
				<li>b->ESCP_DwemerNames/DwemerName_B</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
